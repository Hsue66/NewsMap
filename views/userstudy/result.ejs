<%# /***********************************************************************
  TopicMap : Web application for visualizing a news map generated by TopicMaps
  Authors: Sumin Hong (hsue0606@gmail.com), U Kang (ukang@snu.ac.kr)
  Data Mining Lab., Seoul National University

-------------------------------------------------------------------------
File: result.ejs
 - A layout file for userstudy result page of TopicMap.
 - a page that shows userstudy result.
Version: 1.0
***********************************************************************/ %>

<% include ../partial/header %>
<link rel="stylesheet" href="/ucss/common.css">
<% include ../partial/nav %>

<div class="body_contents">
<div class="container">
  <div class="jumbotron question">
    <h1>Result</h1>
  </div>

  <div class="container">
    <%
    for(var topic in data){%>
      <h2><%=topic%></h2>
      <%
      for(var dataset in data[topic]){ %>
        <div class="col-sm-6">
          <h3><%=dataset%></h3>
          <h4>best :<%=data[topic][dataset].best%></h4>
          <h4>incohA : <%=data[topic][dataset].incohA%></h4>
          <h4>redA : <%=data[topic][dataset].recurA%></h4>
          <h4>redT : <%=data[topic][dataset].recurT%></h4>
          <h4>connA : <%=data[topic][dataset].connA%></h4>
        </div>
    <%}
    }
    %>
  </div>

</div>

<% include ../partial/footer %>
